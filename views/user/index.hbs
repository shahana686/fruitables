<script>
document.addEventListener('DOMContentLoaded', () => {
  const buttons = document.querySelectorAll('.add-to-cart-btn');
  const alertBox = document.getElementById('cart-alert');

  buttons.forEach(btn => {
    btn.addEventListener('click', async () => {
      const productId = btn.getAttribute('data-id');

      try {
        const res = await fetch(`/user/add-to-cart/${productId}`, {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
            'Accept': 'application/json'
          },
            credentials: 'include' // ‚úÖ VERY IMPORTANT

        });
         // üëá Handle unauthorized response
      if (res.redirected && res.url.includes('/user/login')) {
  window.location.href = res.url;
  return;
}

        const data = await res.json();
        
        if (data.success) {
          alertBox.textContent = data.message;
          alertBox.classList.remove('d-none');
          alertBox.classList.remove('alert-danger');
          alertBox.classList.add('alert-success');

          // ‚úÖ Update cart count
          const cartCountElem = document.getElementById('cart-count');
          if (cartCountElem) {
            cartCountElem.textContent = data.cartCount;
          }
          setTimeout(() => alertBox.classList.add('d-none'), 2000);
        } else {
          alertBox.textContent = "Failed to add item.";
          alertBox.classList.remove('d-none');
          alertBox.classList.remove('alert-success');
          alertBox.classList.add('alert-danger');
          setTimeout(() => alertBox.classList.add('d-none'), 2000);
        } 
      } catch (err) {
        alertBox.textContent = "Error adding to cart.";
        alertBox.classList.remove('d-none');
        alertBox.classList.remove('alert-success');
        alertBox.classList.add('alert-danger');
        setTimeout(() => alertBox.classList.add('d-none'), 2000);
      }
    });
  });
});
</script>

 <script>
